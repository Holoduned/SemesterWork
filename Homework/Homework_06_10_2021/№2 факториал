using System;

namespace study
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(Factorial(Convert.ToInt32(Console.ReadLine())));
        }

        static int Factorial(int n)
        {
            if (n == 0)
            {
                return 1;
            }

            else
            {
                return n * Factorial(n - 1);
            }
        }
    }
}